{
   "background": {
      "persistent": false,
      "scripts": [ "node_modules/idb-keyval/dist/idb-keyval-min.js", "scripts/general.js", "scripts/eventPage.js" ]
   },
   "browser_action": {
      "default_popup": "ElementLocator.html",
      "default_title": "Element Locator"
   },
   "content_scripts": [ {
      "js": [ "scripts/general.js", "scripts/contentScript.js" ],
      "matches": [ "http://*/*", "https://*/*" ]
   } ],
   "description": "Locate and save web elements and get their xpath/cssSelector code",
   "icons": {
      "128": "images/elementsLocator128.png",
      "16": "images/elementsLocator16.png",
      "48": "images/elementsLocator48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Element Locator",
   "permissions": [ "activeTab", "contextMenus" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.8",
   "web_accessible_resources": [ "css/general.css" ]
}
